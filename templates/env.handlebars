const env = process.env;

{{#each envs}}
{{#if comment}}

/** {{ comment }} */
{{/if}}
{{#switch type}}
{{#case 'string'}}
export const {{ name }} = env.{{ name }};
{{/case}}
{{#case 'boolean'}}
export const {{ name }} = env.{{ name }} === 'true';
{{/case}}
{{#case 'number'}}
export const {{ name }} = parseFloat(env.{{ name }});
{{/case}}
{{/switch}}
{{/each}}